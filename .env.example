# Copy to .env and fill
GROQ_API_KEY=your_groq_key_here

# Where raw SEC files will be downloaded
SEC_DOWNLOAD_DIR=./data/raw/sec

# DuckDB database file
DUCKDB_PATH=./data/sql/finance.duckdb

# Embedding + reranker models (local HF; free)
EMBED_MODEL=intfloat/e5-base-v2
RERANKER_MODEL=BAAI/bge-reranker-base

# Default Groq chat model
GROQ_CHAT_MODEL=llama-3.3-70b-versatile

SEC_COMPANY=TrustGraphRAG
SEC_EMAIL=you@yourdomain.com